{
  "name": "@ericbiewener/utils",
  "version": "3.3.7-alpha.0",
  "description": "Some very self-centered utilities.",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/ericbiewener/utlz",
  "author": "Eric Biewener",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublish": "yarn build",
    "build": "rm -rf dist && tsc",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,json,yml}\"",
    "eslint": "eslint --fix --ext .ts,.tsx ./src",
    "test": "jest",
    "test:watch": "yarn run test --watch",
    "tw": "yarn run test:watch"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "execa": "4.0.3",
    "fuzzy": "0.1.3"
  },
  "gitHead": "beb3a66148a26775883feeed2275e18abc883837"
}
